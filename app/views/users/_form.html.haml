- form_for @user do |f| 
  = f.error_messages 
  .input
    = f.label :email 
    = f.text_field :email 
  
  .input
    = f.label :password 
    = f.password_field :password 
  
  .input
    = f.label :password_confirmation 
    = f.password_field :password_confirmation 
  
  %p= f.submit "Submit" 
